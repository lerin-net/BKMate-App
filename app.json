{
  "expo": {
    "name": "BKMate",
    "slug": "BKMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/01-logobachkhoasang-1@3x.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/01-logobachkhoasang-1@3x.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },

    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "useCleartextTraffic": true
      }      
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        // "projectId": "55f3ac9d-7989-473b-89a2-f5a500071ec7"
        "projectID": "f1dfcced-2aaa-43f0-8cc9-6a20c55b8713"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ]
  }
}
